<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.knoldus</groupId>
    <artifactId>project-to-test</artifactId>
    <version>1.0-SNAPSHOT</version>
    <packaging>jar</packaging>
    <name>Test Codesquad</name>

    <dependencies>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.11</version>
            <scope>test</scope>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <artifactId>codesquad</artifactId>
                <configuration>
                    <organisationName>TestOrg5</organisationName>
                    <projectName>Testproj3</projectName>
                    <moduleName>TestMod1</moduleName>
                    <files>
                        <file>/home/knoldus/Downloads/scoverage.xml</file>
                    </files>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>